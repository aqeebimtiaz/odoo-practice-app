      <console name="Al Baraka" type="STANDARD" schema="public" session="Main"><![CDATA[select pt."name", cost_value, sale_value from
(
select pol.product_id as product_id,
sum(pol.purchase_price * qty) as cost_value, 
sum(pol.price_unit * qty) as sale_value
from pos_order_line as pol
left join pos_order as po on pol.order_id = po.id
left join pos_session as ps on po.session_id = ps.id
where po.state in ('paid', 'done', 'invoiced')
        and po.location_id = 18
        and date_trunc('day',po.date_order) >= '2019-04-13'
        and date_trunc('day',po.date_order) <= '2019-04-23'
group by pol.product_id
) tbl_line 
inner join product_product as pp 
            on tbl_line.product_id = pp.id
inner join product_template as pt 
        on pp.product_tmpl_id = pt.id 


select pol.product_id,
pol.purchase_price,
pol.qty, 
pol.price_unit
from pos_order_line as pol
left join pos_order as po on pol.order_id = po.id
left join pos_session as ps on po.session_id = ps.id
where po.state in ('paid', 'done', 'invoiced')
        and po.location_id = 18
        and date_trunc('day',po.date_order) >= '2019-04-13'
        and date_trunc('day',po.date_order) <= '2019-04-23'
         and pol.product_id = 22188;

select pt.name as product_name, pol.product_id,
    sum(purchase_price * qty) as product_purchase_price, 
    sum(price_unit * qty) as sell_price 
        from pos_order_line as pol 
    left join pos_order as po on pol.order_id = po.id
    left join pos_session as ps on po.session_id = ps.id
    left join product_product as pp 
            on pol.product_id = pp.id
    left join product_template as pt 
        on pp.product_tmpl_id = pt.id 
    where po.state in ('paid', 'done', 'invoiced')
        and po.location_id = 18
        and date_trunc('day',po.date_order) >= 'Apr 13, 2019, 3:24:24 PM'
        and date_trunc('day',po.date_order) <= 'Apr 23, 2019, 3:24:24 PM'
        and ps.config_id = 9
    group by product_id, pt.name
    order by product_purchase_price asc;

select pt.name as product_name,pp.barcode,pc.name as category,
            sq.qty, (sq.qty*ip.value_float) as cost_price,pu.name as uom from stock_quant sq
            left join ir_property ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq
            .product_id))
            left join product_product pp on(sq.product_id=pp.id)
            left join product_template pt on(pp.product_tmpl_id=pt.id)
            left join product_category pc on(pt.categ_id=pc.id)
            left join product_uom pu on(pt.uom_id=pu.id)
            where sq.location_id=18 order by pc.name
            
select
    pt.name as product_name,
    pp.barcode as barcode,
    pc.name as category,
    sq.qty as qty,
    COALESCE(ip.value_float,0) as cost_price,
    pu.name as uom,
    COALESCE((sq.qty*ip.value_float),0) as Total_cost_price
        from
            stock_quant as sq
            left join ir_property ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq.product_id))
            left join product_product as pp on sq.product_id=pp.id
            left join product_template as pt on pp.product_tmpl_id=pt.id
            left join product_category as pc on pt.categ_id=pc.id
            left join product_uom as pu on pt.uom_id=pu.id
        where
--            sq.location_id= and 
            sq.product_id = 22531
         group by 
         sq.product_id,pt."name",pp.barcode,pc."name",sq.qty,ip.value_float,pu."name"
        order by
            pc.name
            
select  from (
    select 
    sum(sq.qty), 
    max(COALESCE(ip.value_float,0)) as cost_price,
    sum(COALESCE((sq.qty*ip.value_float),0)) as Total_cost_price
        from
            stock_quant as sq
    left join ir_property ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq.product_id))
    where sq.product_id = 22531
    group by sq.product_id) as tbl1
            left join product_product as pp on sq.product_id=pp.id
            left join product_template as pt on pp.product_tmpl_id=pt.id
            left join product_category as pc on pt.categ_id=pc.id
            left join product_uom as pu on pt.uom_id=pu.id
        where
--            sq.location_id=3 and 
            sq.product_id = 22531
        order by
            pc.name

select 
    sq.qty, 
    max(COALESCE(ip.value_float,0)) as cost_price,
    sum(COALESCE((sq.qty*ip.value_float),0)) as Total_cost_price
        from
            stock_quant as sq
    left join ir_property ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq.product_id))
    where sq.product_id = 22531
    group by sq.product_id
    


select sl.id, sl.name from (select distinct sq.location_id as location  from stock_quant as sq)tbl1
    inner join  stock_location as sl on tbl1.location = sl.id;
    
select * from product_template where id = 10442;
select standard_price from product_product;
select * from product_product where id = 10442;

select  * from ir_property where name = 'standard_price';
select * from product_uom;
select id, name from stock_location

select sl.id, sl.name from stock_location as sl inner join stock_quant

select
    pt.name as Product_name,
    pp.barcode as Barcode,
    pc.name as Category,
    tbl2.qty,
    pu.name as UOM,
    tbl2.cost_price,
    tbl2.total_cost_price
from(
    select tbl1.productID, 
	sum(tbl1.qty) as qty,
	MAX(tbl1.cost_price) as cost_price,
	sum(tbl1.total_cost_price) as total_cost_price
    from(
        select 
            sq.product_id as productID,
            sq.qty,
            COALESCE(ip.value_float,0) as cost_price,        
            COALESCE((sq.qty*ip.value_float),0) as total_cost_price  
        from 
            stock_quant as sq
        left join ir_property as ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq.product_id))
        where sq.location_id in (18, 15)
--    where sq.product_id=22531
    ) as tbl1
    group by tbl1.productID) tbl2
    left join product_product as pp on tbl2.productID=pp.id 
    left join product_template as pt on pp.product_tmpl_id=pt.id
    left join product_category as pc on pt.categ_id=pc.id
    left join product_uom as pu on pt.uom_id=pu.id
    
    order by pc.name
     ;



select
    pt.name as Product_name,
    pp.barcode as Barcode,
    pc.name as Category,
    tbl2.qty,
    pu.name as UOM,
    tbl2.cost_price,
    tbl2.total_cost_price
from
    (select  tbl1.productID, 
        sum(tbl1.quant) as qty, 
        MAX(tbl1.cost_price) as cost_price, 
        sum(tbl1.total_cost_price) as total_cost_price 
    from
        (select 
            sq.product_id as productID,
            sum(sq.qty) as quant,
            max(COALESCE(ip.value_float,0)) as cost_price,
            sum(round(COALESCE((sq.qty*ip.value_float),0))) as total_cost_price  
        from 
            stock_quant as sq
        left join ir_property as ip on (ip.name = 'standard_price' and ip.res_id = concat('product.product,',sq.product_id))
            where sq.cost = 0 and sq.location_id in (18, 15)
            group by sq.product_id
        
        UNION ALL
        
        select 
            sq.product_id as productID,
            sum(sq.qty) as quant,
            max(COALESCE(sq.cost,0)) as cost_price,
           sum(round(COALESCE((sq.qty*sq.cost),0))) as total_cost_price  
        from 
            stock_quant as sq
            where sq.cost != 0 and sq.location_id in (18, 15)
            group by sq.product_id
        ) tbl1
        group by tbl1.productID) tbl2
        left join product_product as pp on tbl2.productID=pp.id 
    left join product_template as pt on pp.product_tmpl_id=pt.id
    left join product_category as pc on pt.categ_id=pc.id
    left join product_uom as pu on pt.uom_id=pu.id
    
    order by pc.name

